extends layout

block content
 each val in data
  
  form(action='updateData' method='get')
   .row
    .input-field.col.s6
     input#_id.validate(type='hidden',name='_id',value=val._id)
   .row
    .input-field.col.s6
     label(for='name') Name
     input#name.validate(type='text',name='name',value=val.name)
   .row
    .input-field.col.s6
     label(for='phoneNo') Phone No
     input#phoneNo.validate(type='text',name='phoneNo',value=val.phoneNo)
   .row
    button.btn(type='submit') Update
